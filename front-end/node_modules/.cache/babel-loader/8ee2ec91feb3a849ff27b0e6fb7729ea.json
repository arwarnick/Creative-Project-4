A,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA;AAFA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AASA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AATA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAjBA;AAkBA,IAAA,QAlBA,sBAkBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA1BA;AA2BA,IAAA,UA3BA,sBA2BA,IA3BA,EA2BA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA9BA;AA+BA,IAAA,UA/BA,sBA+BA,IA/BA,EA+BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAxCA;AAyCA,IAAA,QAzCA,oBAyCA,IAzCA,EAyCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AADA,iBAAA,CAFA;;AAAA;AAKA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AANA,kDAOA,IAPA;;AAAA;AAAA;AAAA;AASA,gBAAA,OAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA;AApDA;AAvBA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n  <div class=\"section-1\">\n\t<div class=\"heading\">\n\t\t<h2>Add Car to Inventory</h2>\n\t</div>\n\t<div class=\"add\">\n\t\t<div class=\"form\">\n\t\t\t<input v-model=\"title\" placeholder=\"Make/Model\">\n\t\t\t<p></p>\n\t\t\t<input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n\t\t\t<button @click=\"upload\">Upload</button>\n\t\t</div>\n\t\t<div class=\"upload\" v-if=\"addItem\">\n\t\t\t<h2>{{addItem.title}}</h2>\n\t\t\t<img :src=\"addItem.path\" />\n\t\t</div>\n\t</div>\n  </div>\n  <div class=\"section-2\">\n\t<div class=\"heading\">\n\t\t<h2>Edit Inventory</h2>\n\t</div>\n\t<div class=\"edit\">\n\t\t<div class=\"form\">\n\t\t\t<input v-model=\"findTitle\" placeholder=\"Search Inventory\">\n\t\t\t<div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n\t\t\t\t<div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"upload\" v-if=\"findItem\">\n\t\t\t<input v-model=\"findItem.title\">\n\t\t\t<p></p>\n\t\t\t<img :src=\"findItem.path\" />\n\t\t</div>\n\t\t<div class=\"actions\" v-if=\"findItem\">\n\t\t\t<button @click=\"deleteItem(findItem)\">Delete</button>\n\t\t\t<button @click=\"editItem(findItem)\">Edit</button>\n\t\t</div>\n\t</div>\n  </div>\n</div>\n</template>\n\n<style scoped>\n.admin {\n  display: flex;\n  justify-content: space-around;\n}\n\n.admin h2 {\n  font-family: \"Arial Black\", Gadget, sans-serif;\n}\n\n.admin p {\n  font-family: \"Arial Black\", Gadget, sans-serif;\n}\n\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n  font-family: \"Arial Black\", Gadget, sans-serif;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n  flex-direction: column;\n}\n\n/* Form */\ninput,\ntextarea,\nselect {\n  font-family: \"Arial Black\", Gadget, sans-serif;\n  font-size: 1em;\n}\n\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\n</style>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Admin',\n  data() {\n    return {\n    title: \"\",\n    file: null,\n    addItem: null,\n    items: [],\n    findTitle: \"\",\n    findItem: null,\n    }\n  },\n  computed: {\n    suggestions() {\n      let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n      return items.sort((a, b) => a.title > b.title);\n    }\n  },\n  \n  created() {\n    this.getItems();\n  },\n\n  methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    async upload() {\n      try {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          path: r1.data.path\n        });\n        this.addItem = r2.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}